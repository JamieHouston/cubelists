<configuration>
  <system.webServer>

    <!-- indicates that the hello.js file is a node.js application 
    to be handled by the iisnode module -->

    <handlers>
    	<remove name="iisnode"/>
    	<add name="iisnode" path="two-way-server.js" verb="*" modules="iisnode" />
    </handlers>
  </system.webServer>
</configuration>